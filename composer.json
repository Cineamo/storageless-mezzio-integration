{
    "name":        "psr7-sessions/storageless-zend-expressive-integration",
    "description": "Storageless PSR-7 Session support for Zend Expressive",
    "license":     "MIT",
    "authors": [
        {
            "name":     "Marco Pivetta",
            "email":    "ocramius@gmail.com",
            "homepage": "http://ocramius.github.io/",
            "role":     "Developer"
        },
        {
            "name":  "Jefersson Nathan",
            "email": "malukenho@phpse.net",
            "role":  "Developer"
        },
        {
            "name":  "Luís Otávio Cobucci Oblonczyk",
            "email": "lcobucci@gmail.com",
            "role":  "Developer"
        }
    ],
    "require": {
        "php":                                   "^7.2",
        "psr7-sessions/storageless":             "^5.0.0",
        "zendframework/zend-expressive-session": "^1.2.1",
        "psr/http-message":                      "^1.0.1",
        "lcobucci/clock":                        "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit":                  "^7.3.1",
        "zendframework/zend-diactoros":     "^1.8.5",
        "infection/infection":              "^0.10.0",
        "phpstan/phpstan":                  "^0.10.2",
        "phpstan/phpstan-phpunit":          "^0.10",
        "phpstan/phpstan-strict-rules":     "^0.10.1",
        "squizlabs/php_codesniffer":        "^3.3.1",
        "doctrine/coding-standard":         "^4.0.0",
        "maglnet/composer-require-checker": "^2.0.0"
    },
    "autoload": {
        "psr-4": {
            "PSR7Sessions\\ZendExpressive\\": "src/ZendExpressive"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PSR7SessionsTest\\ZendExpressive\\": "test/ZendExpressive"
        }
    }
}
